{
  "name": "agneto",
  "version": "0.3.0",
  "description": "AI-powered autonomous coding agent with human-in-the-loop planning",
  "type": "module",
  "bin": {
    "agneto": "./dist/cli.js"
  },
  "files": [
    "dist",
    "bin",
    "package.json",
    "README.md"
  ],
  "keywords": [
    "ai",
    "agent",
    "development",
    "coding",
    "automation",
    "claude",
    "llm",
    "agile",
    "autonomous"
  ],
  "author": "DraÅ¾en Urch",
  "license": "Source Available",
  "repository": {
    "type": "git",
    "url": "https://github.com/durch/agneto.git"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "tsc -p . && npm run copy-prompts && npm run add-shebang",
    "copy-prompts": "mkdir -p dist/prompts && cp -r src/prompts/*.md dist/prompts/",
    "add-shebang": "echo '#!/usr/bin/env node' | cat - dist/cli.js > temp && mv temp dist/cli.js && chmod +x dist/cli.js",
    "prepublishOnly": "npm run build",
    "start": "tsx src/cli.ts",
    "dashboard": "npx tsx dashboard/server.ts",
    "new-task": "bash scripts/new-task.sh",
    "cleanup-task": "bash scripts/cleanup-task.sh",
    "merge-task": "bash scripts/merge-worktree.sh",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:watch": "vitest --watch",
    "test:git": "vitest test/git",
    "test:run": "vitest run"
  },
  "dependencies": {
    "@anthropic-ai/claude-code": "^1.0.113",
    "@inquirer/prompts": "^7.8.6",
    "ajv": "^8.17.1",
    "boxen": "^8.0.1",
    "chalk": "^5.3.0",
    "cli-highlight": "^2.1.11",
    "commander": "^12.1.0",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "simple-git": "^3.24.0",
    "strip-ansi": "^7.1.0",
    "wrap-ansi": "^9.0.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^24.4.0",
    "@types/ws": "^8.5.12",
    "@vitest/ui": "^3.2.4",
    "tsx": "^4.19.0",
    "typescript": "^5.6.3",
    "vitest": "^3.2.4"
  }
}
