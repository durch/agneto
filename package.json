{
  "name": "agneto",
  "version": "0.1.0",
  "description": "AI-powered autonomous coding agent with human-in-the-loop planning",
  "type": "module",
  "bin": {
    "agneto": "./bin/agneto.js"
  },
  "files": [
    "dist",
    "bin",
    "package.json",
    "README.md"
  ],
  "keywords": [
    "ai",
    "agent",
    "development",
    "coding",
    "automation",
    "claude",
    "llm",
    "agile",
    "autonomous"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/durch/agneto.git"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "tsc -p .",
    "prepublishOnly": "npm run build",
    "start": "tsx src/cli.ts",
    "new-task": "bash scripts/new-task.sh",
    "cleanup-task": "bash scripts/cleanup-task.sh",
    "merge-task": "bash scripts/merge-worktree.sh",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:watch": "vitest --watch",
    "test:git": "vitest test/git",
    "test:run": "vitest run"
  },
  "dependencies": {
    "@anthropic-ai/claude-code": "^1.0.113",
    "@inquirer/prompts": "^7.8.6",
    "ajv": "^8.17.1",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "simple-git": "^3.24.0"
  },
  "devDependencies": {
    "@types/node": "^24.4.0",
    "@vitest/ui": "^3.2.4",
    "tsx": "^4.19.0",
    "typescript": "^5.6.3",
    "vitest": "^3.2.4"
  }
}
